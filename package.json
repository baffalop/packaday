{
  "name": "packaday",
  "description": "A datepicker with a puzzling UI",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm dune && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "pnpm build && gh-pages -d dist",
    "dune": "opam exec -- dune build @app",
    "dune:watch": "pnpm dune --watch"
  },
  "packageManager": "pnpm@10.19.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "eslint": "^9.39.2",
    "gh-pages": "^6.3.0",
    "neostandard": "^0.12.2",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vite-plugin-melange": "^3.10.0"
  }
}
